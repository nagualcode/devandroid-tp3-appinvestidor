<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.infnet.appInvestidor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tp3androidperfilinvestidor">
        <activity android:name="br.edu.infnet.appInvestidor.navigation.NavigateActivity"></activity>
        <activity android:name="br.edu.infnet.appInvestidor.ui.activities.ResultActivity" />
        <activity android:name="br.edu.infnet.appInvestidor.ui.activities.FormQuestionsActivity" />
        <activity android:name="br.edu.infnet.appInvestidor.ui.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>